@-webkit-keyframes Login-background-gradient {
    0%{background-position:86% 0%}
    50%{background-position:15% 100%}
    100%{background-position:86% 0%}
}
@-moz-keyframes Login-background-gradient {
    0%{background-position:86% 0%}
    50%{background-position:15% 100%}
    100%{background-position:86% 0%}
}
@keyframes Login-background-gradient {
    0%{background-position:86% 0%}
    50%{background-position:15% 100%}
    100%{background-position:86% 0%}
}

.animated-backgroud-gradient{
    background: linear-gradient(220deg, #e46cdb, #b96ce4, #6ce4c0, #91ee9d, #d96ce4, #c95dd4);
    background-size: 1200% 1200%;
    -webkit-animation: Login-background-gradient 16s ease infinite;
    -moz-animation: Login-background-gradient 16s ease infinite;
    animation: Login-background-gradient 16s ease infinite;
}

