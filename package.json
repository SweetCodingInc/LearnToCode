{
  "name": "learn-to-code",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "client": "ng serve --open",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:compile": "tsc -p ./server --watch",
    "server:build": "tsc -p ./server",
    "server:run": "nodemon ./server/dist",
    "dev": "npm run server:build && concurrently -k \"npm run server:compile\" \"npm run server:run\" \"npm run client\"",
    "predev": "npm install",
    "prestart": "npm install",
    "start": "npm run server:build && npm run build",
    "docs": "typedoc --includes ./server/src --out ./docs",
    "seed": "node seed/seed.js",
    "setup:prod": "npm install && npm run seed && npm start",
    "setup:dev": "npm install && npm run seed && npm run dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@types/q": "^1.5.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "jquery": "^3.3.1",
    "moment": "^2.22.0",
    "mongoose": "^5.0.13",
    "popper.js": "^1.14.3",
    "q": "^1.5.1",
    "restify": "^7.1.0",
    "restify-cors-middleware": "^1.1.0",
    "restify-router": "^0.5.0",
    "rxjs": "^5.5.6",
    "tsdoc": "0.0.4",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/axios": "^0.14.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/mongoose": "^5.0.9",
    "@types/node": "^6.0.104",
    "@types/restify": "^5.0.7",
    "@types/restify-cors-middleware": "^1.0.1",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "docdash": "^0.4.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsdoc": "^3.5.5",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.17.3",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "~2.5.3"
  }
}
